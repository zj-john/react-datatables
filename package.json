{
  "name": "react-datatable-jq",
  "version": "2.0.0",
  "description": "Datatable components built with React and JQuery",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --presets=env,react,stage-2 --copy-files",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/react-datatable-jq.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/react-datatable-jq.min.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zj-john/react-datatables.git"
  },
  "keywords": [
    "react",
    "datatable"
  ],
  "author": "zj_john",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zj-john/react-datatables/issues"
  },
  "homepage": "https://github.com/zj-john/react-datatables",
  "dependencies": {
    "datatables.net": "^1.10.16",
    "jquery": "^3.2.1",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.8",
    "file-loader": "^1.1.6",
    "script-loader": "^0.7.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "peerDependencies": {
    "jquery": ">=2.1.4",
    "react": "^0.14.0 || ^15.0.0-rc || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0-rc || ^15.0.0 || ^16.0.0"
  }
